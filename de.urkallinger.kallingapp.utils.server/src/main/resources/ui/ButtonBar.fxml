<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.HBox?>

<ButtonBar xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.urkallinger.kallingapp.utils.server.controller.ButtonBarController">
	<buttons>
		<Button ButtonBar.buttonData="LEFT" fx:id="btnStart"    onAction="#onStart" />
		<Button ButtonBar.buttonData="LEFT" fx:id="btnStop"     onAction="#onStop" />
		<Button ButtonBar.buttonData="LEFT" fx:id="btnRestart"  onAction="#onRestart" />
		<Button ButtonBar.buttonData="LEFT" fx:id="btnClear"    onAction="#onClear" />
		<Button ButtonBar.buttonData="LEFT" fx:id="btnUpdate"   onAction="#onUpdate" />
		<Button ButtonBar.buttonData="LEFT" fx:id="btnSettings" onAction="#onSettings" />
		<HBox />
	</buttons>
	<padding>
		<Insets left="10.0" right="10.0" top="10.0" />
	</padding>
</ButtonBar>
